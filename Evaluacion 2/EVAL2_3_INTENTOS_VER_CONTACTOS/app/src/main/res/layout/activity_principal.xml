<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.example.vanessatrevizo.eval2_3_intentos_ver_contactos.Principal">

    <Button
        android:id="@+id/btnBuscarContacto"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="onClick"
        android:text="Mostrar contactos" />

    <EditText
        android:id="@+id/editTextBus"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName" />

    <Button
        android:id="@+id/btnBuscar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="otroClick"
        android:text="Buscar " />

    <Button
        android:id="@+id/btnImagenes"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:onClick="otromasClick"
        android:text="Imagenes" />

</LinearLayout>
